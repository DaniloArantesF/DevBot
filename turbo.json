{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "bot#build": {
      "env": [
        "NODE_ENV",
        "DISCORD_TOKEN",
        "DISCORD_CLIENT_ID",
        "DISCORD_CLIENT_SECRET",
        "DISCORD_GUILD_ID",
        "DISCORD_TEST_GUILD_ID"
      ]
    },
    "web#build": {
      "env": ["SECRET_COOKIE_PASSWORD"]
    }
  },
  "globalDependencies": [".env", "package.json", "tsconfig.json"]
}
